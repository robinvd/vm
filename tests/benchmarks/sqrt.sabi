fn sqrt(n) {
    let x = pow(2, 31)
    let y = 0
    while True {
        y = floor(div(
            add(x, floor(div(n,x))),
            2
        ))
        if geq(y,x) {
            return x
        }
        x = y
    }
}

fn main() {
    let res = 10485760000
    let i = 1
    let sum = 0
    
    while leq(i, 100) {
        i = add(i, 1)
        sum = add(sum, sqrt(res))
    }

    print(sum)
}